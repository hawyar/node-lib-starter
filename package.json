{
  "name": "esm-pkg",
  "version": "1.0.0",
  "description": "Base esm pkg",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "tap --no-coverage --reporter=specy -J --color",
    "coverage": "npm test -- --coverage-report=html",
    "lint": "eslint . --ext .js",
    "build:docs": "rm -rf docs && jsdoc index.js -t ./node_modules/foodoc/template -d docs -r -e utf8 --readme README.md -c conf.json",
    "serve:docs": " rm -rf docs && npm run build:docs && node static",
    "build:node": "esbuild index.js --format=cjs --bundle --platform=node,--outdir=build --minify",
    "build:binary": "pkg index.js -o build/parser",
    "build": "npm run build:node && npm run build:binary"
  },
  "author": "hawyar",
  "dependencies": {
    "esbuild": "^0.12.20"
  },
  "devDependencies": {
    "finalhandler": "^1.1.2",
    "foodoc": "^0.0.9",
    "jsdoc": "^3.6.7",
    "pkg": "^5.3.1",
    "tap": "^15.0.9"
  }
}
